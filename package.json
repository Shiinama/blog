{
  "name": "blog",
  "version": "0.1.0",
  "author": "felix",
  "description": "A minimal and cutting-edge AI project template for validating business ideas",
  "topics": [
    "ai",
    "nextjs",
    "react",
    "openai",
    "drizzle",
    "typescript",
    "business-validation",
    "chat-application",
    "template"
  ],
  "keywords": [
    "easy",
    "template",
    "next.js",
    "react",
    "chat",
    "ai",
    "openai",
    "openrouter",
    "drizzle",
    "nextauth",
    "typescript"
